{
  "name": "strider-runner-core",
  "version": "2.0.1",
  "description": "Just run those jobs. Decoupled from load balancing, joq queues, etc.",
  "main": "index.js",
  "scripts": {
    "lint": "jshint .",
    "test-only": "mocha -R spec",
    "pretest": "npm run-script lint",
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "posttest": "istanbul check-coverage",
    "prepublish": "npm test && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-runner-core.git"
  },
  "keywords": [
    "strider",
    "worker",
    "testing",
    "ci",
    "continuous",
    "deployment"
  ],
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "chai": "~3.5.0",
    "jshint": "~2.9.2",
    "mocha": "~2.5.3",
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.3"
  },
  "dependencies": {
    "mkdirp": "~0.3.5",
    "lodash": "~1.3.1",
    "async": "~0.2.9",
    "shell-quote": "^1.6.0"
  }
}
